{
    "comments": {
        "lineComment": "#",
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"],
        ["do", "end"],
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"],
        ["do", "end"]
    ],
    "folding": {
      "markers": {
        "start": "^\\s*#\\s*region\\b",
        "end": "^\\s*#\\s*endregion\\b"
      },
    },
    "indentationRules": {
        "increaseIndentPattern": "^.*\\b(do|\\{|\\[|\\()\\b.*$",
        "decreaseIndentPattern": "^\\s*\\b(end|\\}|\\]|\\))\\b.*$"
    },
    "wordPattern": "[a-zA-Z_$?][a-zA-Z0-9_$?]*"
}