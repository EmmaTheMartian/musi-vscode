{
	"If statement": {
		"prefix": "if",
		"body": [
			"if ${1:condition} do",
			"$2",
			"end"
		]
	},
	"If-else statement": {
		"prefix": "ife",
		"body": [
			"if ${1:condition} do",
			"$2",
			"end else do",
			"",
			"end"
		]
	},
	"While statement": {
		"prefix": "while",
		"body": [
			"while ${1:condition} do",
			"$2",
			"end"
		]
	},
	"Function declaration": {
		"prefix": "fn",
		"body": [
			"fn ${1:params} do",
			"$2",
			"end"
		]
	},
	"Let function": {
		"prefix": "letfn",
		"body": [
			"let ${1:name} = fn ${2:params} do",
			"$3",
			"end"
		]
	}
}